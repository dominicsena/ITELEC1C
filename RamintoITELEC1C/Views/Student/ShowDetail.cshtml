@{
}
@model Student;

<h1>Instructor Details</h1>

<table class="table table-dark">
    <tr>
        <th>Id</th>
        <td>@Model.StudentId</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.StudentName</td>
    </tr>
    <tr>
        <th>Course</th>
        <td>@Model.Course</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Date Enrolled</th>
        <td>@Model.DateEnrolled</td>
    </tr>
</table>